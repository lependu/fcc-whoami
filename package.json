{
  "name": "fcc-whoami",
  "version": "0.1.0",
  "description": "Experimental dockerized expressjs starter kit project.",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015\"",
    "build": "babel src -s -D --minified -d dist",
    "start": "node dist",
    "lint": "eslint src",
    "test": "nyc --reporter text tape --require babel-register test/*-test.js | tap-spec",
    "coverage": "nyc --reporter html tape --require babel-register test/*-test.js | tap-spec"
  },
  "author": "lependu@protonmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lependu/fcc-whoami.git"
  },
  "private": true,
  "keywords": [
    "express",
    "docker",
    "docker-compose",
    "node",
    "es6",
    "babel",
    "tape",
    "nyc"
  ],
  "dependencies": {
    "body-parser": "1.17.2",
    "express": "4.15.3",
    "morgan": "1.8.2"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.24.1",
    "eslint": "4.2.0",
    "nodemon": "1.11.0",
    "nyc": "^11.0.3",
    "supertest": "3.0.0",
    "tap-spec": "4.1.1",
    "tape": "4.7.0"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 1,
      "no-unused-vars": 1
    }
  }
}
